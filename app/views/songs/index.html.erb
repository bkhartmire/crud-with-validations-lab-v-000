<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <%=song.title%>
        <%=song.artist_name%>
        <%if song.released%>
          <%=song.release_year%>
        <%else%>
          Never released.
        <%end%>
        <%= link_to "Delete", song, method: :delete, data: {confirm: "Really?"}%>
        <%= link_to 'Show', song %>
        <%= link_to 'Edit', edit_song_path(song) %>
      </tr>
    <%end%>
  </tbody>
</table>
